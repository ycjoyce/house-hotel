{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": 0
    },
    "settings": {
        "import/resolver": {
            "alias": {
                "map": [
                    ["@src", "./src"],
                    ["@img", "./src/img"]
                ]
            }
        }
    }
}